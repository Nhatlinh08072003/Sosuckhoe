@model List<SoSucKhoe.Models.Main.Lichtiem>
@{
    ViewData["Title"] = "Thống kê tiêm chủng";
}
<link rel="stylesheet" href="~/css/thongketiemchung.css">

<body>
    <h1 class="title-statistics-vaccin">Thống kê tiêm chủng</h1>
    @foreach (var item in Model)
    {

        <div class="statistics-vaccin">

            <div class="info">
                <p class="name" style="font-weight: 700;">Loại Vắc-xin: </p>
                <p class="vaccine-statistics-vaccin">@item.LoaiVacxin</p>
            </div>

            <div class="info-statistics-vaccin">
                <p class="name" style="font-weight: 700;">Họ và tên: </p>
                <p class="name-user"> @item.Hoten</p>
            </div>

            <div class="info-statistics-vaccin">
                <p class="address" style="font-weight: 700;">Địa chỉ tiêm: </p>
                <p class="address-user"> @item.DiadiemTiem</p>
            </div>

            <div class="info-statistics-vaccin">
                <p class="date" style="font-weight: 700;">Ngày tiêm: </p>
                <p class="date-user">@item.NgayTiem</p>
                <button class="button-detail"><a href="/chitietlichtiem/@item.Id"> Xem chi tiết</a></button>
            </div>


        </div>
    }
</body>